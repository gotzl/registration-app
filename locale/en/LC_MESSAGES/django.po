#: mailing.py:38
#, python-format
msgid "Registration for %s"
msgstr ""

#: mailing.py:39
#, python-format
msgid ""
"mail_body_%(name)s_%(event)s_%(hold_back_hours)i_%(confirm_url)s"
msgstr ""
"Hello %(name)s,\n"
"please confirm the registration for the event %(event)s\n"
"within the next %(hold_back_hours)i by navigating to the following url:\n"
"%(confirm_url)s\n"
"\n"
"\n"
"Thank you!"

#: mailing.py:53
#, python-format
msgid "Reminder of registration for %s"
msgstr ""

#: mailing.py:54
#, python-format
msgid ""
"reminder_body_%(name)s_%(event)s_%(cancel_hours)i_%(confirm_url)s_"
"%(modify_url)s"
msgstr ""
"Hello %(name)s,\n"
"this is a reminder for the upcoming event %(event)s for which you have\n"
"registered. However, your confirmation of the registration is still "
"pending.\n"
"\n"
"Please complete the registration by visiting the following url:\n"
"%(confirm_url)s\n"
"\n"
"If you want to modify or delete your registration, visit\n"
"%(modify_url)s\n"
"\n"
"If not confirmed, your registration will be canceled in %(cancel_hours)i "
"hours.\n"
"\n"
"Thank you!"

#: mailing.py:68
#, python-format
msgid "Confirmed registration for %s"
msgstr ""

#: mailing.py:69
#, python-format
msgid "confirmation_body_%(name)s_%(event)s_%(modify_url)s_%(seats)s"
msgstr ""
"Hello %(name)s,\n"
"\n"
"the registration for the event %(event)s is now confirmed.\n"
"%(seats)s\n"
"If you want to modify your registration, visit\n"
"%(modify_url)s\n"
"\n"
"\n"
"Thank you!"

